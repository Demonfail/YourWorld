            <kind>1</kind>
            <string>///Draw the Info Window

if( InfoA&gt;0.05 ) //If the transparency of the window is under a certain point, skip drawing it
{

    //Set the coordinates for the edges of the info window
    var iwx1=round(view_wview[0]*0.3), iwy1=round((view_hview[0]*0.6)-(InfoDrawH/2));
    var iwx2=round(iwx1+InfoDrawW), iwy2=round(iwy1+InfoDrawH);
    var xx=iwx1+30, yy=iwy1; col=c_black; //Dynamic drawing coordinates
    
    //Draw the window itself
    draw_set_alpha(sqr(InfoA)*0.7);
    draw_set_color(c_black);
    draw_rectangle(iwx1,iwy1,iwx2,iwy2,0);
    
    //Draw the contents of the window
    draw_set_alpha(sqr(InfoA));
    draw_set_color(c_white);
    draw_set_halign(fa_left);
    draw_set_valign(fa_center);
    
    //Title
    draw_set_font(fTwCenL);
    yy+=30;
    draw_text(xx,yy+5,InfoT);
    
    //Table Contents
    draw_set_font(fTwCenM);
    for(i=0;i&lt;=InfoDataMax;i+=1)
    {
        yy+=40;
        
        if( round(i/2)==i/2 ) //If you're on an even number, tint the colour behind it
        {
            draw_set_alpha(sqr(InfoA)*0.2); draw_rectangle(iwx1,yy,iwx2,yy+40,0); draw_set_alpha(sqr(InfoA));
        }
        
        draw_text(xx           ,yy+20,InfoDataCell1[i])
        draw_text(mean(xx,iwx2),yy+20,InfoDataCell2[i])
    }
    
    yy+=50;
    InfoH=yy-iwy1;
    
    //Draw the glowy outline of the window
    draw_sprite_ext(sHMInfoWindowC,0,iwx1  ,iwy1  ,1,1,  0,c_white,InfoA);
    draw_sprite_ext(sHMInfoWindowC,0,iwx1  ,iwy2+1,1,1, 90,c_white,InfoA);
    draw_sprite_ext(sHMInfoWindowC,0,iwx2+1,iwy2+1,1,1,180,c_white,InfoA);
    draw_sprite_ext(sHMInfoWindowC,0,iwx2+1,iwy1  ,1,1,270,c_white,InfoA);
    
    draw_sprite_ext(sHMInfoWindowSH,0,mean(iwx1,iwx2),iwy1,(InfoDrawW-14)/16,1,0,c_white,InfoA);
    draw_sprite_ext(sHMInfoWindowSH,0,mean(iwx1,iwx2),iwy2,(InfoDrawW-14)/16,1,0,c_white,InfoA);
    draw_sprite_ext(sHMInfoWindowSV,0,iwx1,mean(iwy1,iwy2),1,(InfoDrawH-14)/16,0,c_white,InfoA);
    draw_sprite_ext(sHMInfoWindowSV,0,iwx2,mean(iwy1,iwy2),1,(InfoDrawH-14)/16,0,c_white,InfoA);
    
    draw_set_color(c_white);
    draw_set_alpha(1);
}
