        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Do load/save after

// Save game
if (LoadSave == 1)
    {
    var filename = get_save_filename("YoYo City Maps|*.city", LastFileName);
    if( filename!="" )
        {
        debug("filename="+filename);
        var buff = SaveMap(id);
        LastFileName = filename;
        buffer_save_ext(buff, LastFileName, 0, buffer_tell(buff));
        }
    }


// Load game
else
    {
    var filename = get_open_filename("YoYo City Maps|*.city",LastFileName);
    if( filename!="")
        {
        LoadObjects = ds_list_create();
        LastFileName=filename;
        var buff = buffer_load(LastFileName);
        LoadMap(id,buff);
        
        // Create stuff, this should be better controlled, example: player shouldn't spawn without spawn point
        instance_create(0, 0, objPhysicsController);
        CreateObjectsFromList(LoadObjects);
        instance_create(0, 0, objTrafficSpawner);
        SpawnPlayer();
        }
    }


// Fullscreen (explorer window screws game window)
FullscreenMode(fullscreen);

</string>
          </argument>
        </arguments>
      </action>
