// Convert roadFlag into variables we can read
var northFlag = (roadFlag &amp; 8) &gt; 0; // 1000 = 8
var eastFlag  = (roadFlag &amp; 4) &gt; 0; // 0100 = 4
var southFlag = (roadFlag &amp; 2) &gt; 0; // 0010 = 2
var westFlag  = (roadFlag &amp; 1) &gt; 0; // 0001 = 1

// Draw the compass
draw_sprite(sprCompassButtons, 0+northFlag*4, x,    y-40);
draw_sprite(sprCompassButtons, 1+eastFlag*4,  x+40, y);
draw_sprite(sprCompassButtons, 2+southFlag*4, x,    y+40);
draw_sprite(sprCompassButtons, 3+westFlag*4,  x-40, y);

// Handle interaction, toggle flags
if (mouse_click_rectangle(x, y-40, x+40, y))    northFlag = !northFlag;
if (mouse_click_rectangle(x+40, y, x+80, y+40)) eastFlag  = !eastFlag;
if (mouse_click_rectangle(x, y+40, x+40, y+80)) southFlag = !southFlag;
if (mouse_click_rectangle(x-40, y, x, y+40))    westFlag  = !westFlag;

// Recompile flag
roadFlag = northFlag&lt;&lt;3 | eastFlag&lt;&lt;2 | southFlag&lt;&lt;1 | westFlag;

</string>
